{
"name": "WAM",
"description": "Web Application Mapper",
"version": "1.0",
"manifest_version": 3,
"permissions": [
    "activeTab",
    "scripting",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "tabs",
    "cookies"
],
"host_permissions": ["<all_urls>"],
"options_ui": {
    "page": "wam/options.html",
    "open_in_tab": true
},
"sandbox": {
    "pages": ["wam/sandbox.html"]
},
"action": {
    "default_popup": "popup.html"
}
}